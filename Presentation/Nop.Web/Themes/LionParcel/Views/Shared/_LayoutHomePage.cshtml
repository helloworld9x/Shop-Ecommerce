                                    str += "<tbody>";
                                    str += "<tr>";
                                    str += "<td>" + sttDetails[i].ServiceType + "</td>";
                                    str += "<td>" + sttDetails[i].ProductName + "</td>";
                                    str += "<td>" + sttDetails[i].GrossWt + "</td>";
                                    str += "<td>" + sttDetails[i].Pieces + "</td>";
                                    str += "<td>" + sttDetails[i].CurrentStatus + "</td>";
                                    str += "</tr>";
                                    str += "</tbody>";
                                    str += "</table>";
                                    str += "</br>";
                                    str += "<table class='table'>";
                                    str += "<thead><tr><th>Date</th><th>Tracking Code</th><th>Location</th><th>Remarks</th></tr></thead>";
                                    str += "<tbody>";
                                    for (var j = 0; j < shipmentTravelHistory.length; j++) {
                                        if (shipmentTravelHistory[j].STTNumber == sttnumber) {
                                            str += "<tr>";
                                            str += "<td>" + shipmentTravelHistory[j].Date + "</td>";
                                            str += "<td>" + shipmentTravelHistory[j].TrackCode + "</td>";
                                            str += "<td>" + shipmentTravelHistory[j].Location + "</td>";
                                            str += "<td>" + shipmentTravelHistory[j].Remarks + "</td>";
                                            str += "</tr>";
                                        }
                                    }
                                    str += "</tbody>";
                                    str += "</table>";
                                    str += "</br>";
                                    str += "</br>";
                                    allStr += str;
                                }
                                $(mId + " .modal-body").html(allStr);

                            } else {
                                $(mId + " .modal-body").html('No Record Found');
                            }
                            $(mId + " .modal-title").html("Check Your Parcel");
                            $(mId).modal('show');
                        }
                    },

                    error: function () {
                        alert("Oops. Somethings went wrong.");
                    }
                });

            }

        });

        $("#tariffSearch").on("click", function () {
            var origin = $("select[name='slc-origin']").val();
            var destination = $("select[name='slc-destination']").val();
            var weight = $("input[name='inp-weight']").val();
            if (origin && destination && origin !== destination && weight) {
                var mId = "#tariffSearchModal";
                $.ajax({
                    url: 'https://system.lionexpress.co.id/ewebportal/eLexysTariffService.svc/CalculateTariff?cf_signature=H7KoLR7PTz94uS9/pKuJIJ6PbJM=',
                    async: false,
                    type: "POST",
                    dataType: "json",
                    data: JSON.stringify({ Origin: origin, Destination: destination, Weight: weight }),
                    contentType: "application/json; charset=UTF-8",
                    cache: false,
                    success: function (result) {
                        if (result !== "[]") {
                            var myData = JSON.parse(result);
                            if (myData.ErrorStatus[0]["ErrorMessage"] !== "OK") {
                                $(mId + " .modal-body").html(myData.ErrorStatus[0]["ErrorMessage"]);
