            x2 = x.length > 1 ? '.' + x[1] : '';
            var rgx = /(\d+)(\d{3})/;
            while (rgx.test(x1)) {
                x1 = x1.replace(rgx, '$1' + ',' + '$2');
            }
            return x1 + x2;
        }

        $(".button-add").on("click", function () {
            var tRow = $(this).closest("tr");
            var nRow = tRow.next("tr");
            var cols = $(this).closest("tr").find('input[name="track-col"]');
            var arrs = [];
            $.each(cols, function (i, v) {
                var val = $(v).val();
                if (val && !isNaN(val)) {
                    arrs.push(val);
                }
            });
            if (nRow.hasClass("hide") && arrs.length === 3) {
                nRow.removeClass("hide");
            }
        });

        $("#trackingSearch").on("click", function () {
            var rows = $('#trackingTable').find('tr:not([class="hide"])');
            var rArrs = [];
            $.each(rows, function (i, v) {
                var cols = $(v).find('input[name="track-col"]');
                var arrs = [];
                $.each(cols, function (i, v) {
                    var val = $(v).val();
                    if (val && !isNaN(val)) {
                        arrs.push(val);
                    }
                });
                if (arrs.length === 3) {
                    var str = arrs.join("-");
                    rArrs.push(str);
                }
            });
            if (rArrs.length > 0) {
                var sttbnumber = rArrs.join(",");
                if (sttbnumber)
                    var mId = "#tariffSearchModal";
