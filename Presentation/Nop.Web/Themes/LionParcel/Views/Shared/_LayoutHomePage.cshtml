                                    str += "<tr>";
                                    str += "<td>" + results[i].Product + "</td>";
                                    str += "<td>" + results[i].ServiceType + "</td>";
                                    str += "<td>Rp. " + addCommas("" + results[i].TotalAmountTariff) + "</td>";
                                    str += "<td>" + results[i].TariffSection + "</td>";
                                    str += "</tr>";
                                }
                                str += "</tbody>";
                                str += "</table>";
                                $(mId + " .modal-body").html(str);
                                $(mId + " .modal-title").html("Check Our Tarif");
                                $(mId).modal('show');
                            }
                        }
                    },
                    error: function () {
                        $(mId).modal('show');
                        alert("Oops. Somethings went wrong.");
                    }
                }
                );

            }
        });
    });
