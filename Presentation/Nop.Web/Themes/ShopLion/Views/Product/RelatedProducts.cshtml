@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="related-products-grid product-grid f-d-b">
        <div class="main-title1">
            <h4>@T("Products.RelatedProducts")</h4>
        </div>
        <div class="item-grid row">
            @foreach (var item in Model)
            {
                <div class="item-box col-md-3 mg-5">@Html.Partial("_ProductBox", item)</div>
            }
        </div>
    </div>
}